<?xml version="1.0" encoding="utf-8"?>
<s:WindowedApplication xmlns:fx="http://ns.adobe.com/mxml/2009" 
					   xmlns:s="library://ns.adobe.com/flex/spark" 
					   xmlns:mx="library://ns.adobe.com/flex/mx"
					   preinitialize="vpDude_preinitializeHandler(event)"
					   creationComplete="vpDude_creationCompleteHandler(event)" 
					   width="890" height="600"
					   xmlns:components="components.*">

	<fx:Script>
		<![CDATA[
			
		]]>
	</fx:Script>


	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script source="Main.as" />
	<s:VGroup paddingLeft="10" paddingBottom="10" paddingRight="10" paddingTop="10">
		<s:BitmapImage id="logo" source="@Embed('../assets/vp-logo.png')" />
		
		<mx:TabNavigator id="tabNav" width="870" height="500" change="tabNav_changeHandler(event)" >
			<components:Search/>
			<components:Download id="browserTab" />
			<components:Config/>
			<components:Upload/>
			<components:Quit/>
		</mx:TabNavigator>
		<s:Label id="location" text="{vpUrl}" />
		<s:Label id="lblInfo" text="label" />
	</s:VGroup>
</s:WindowedApplication>
